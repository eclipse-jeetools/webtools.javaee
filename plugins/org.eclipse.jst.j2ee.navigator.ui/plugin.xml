         viewerId="org.eclipse.ui.navigator.ProjectExplorer">
         <includes>
           <contentExtension pattern="org.eclipse.jst.navigator.j2ee.ui.*"/>
           <contentExtension pattern="org.eclipse.jst.j2ee.navigator.ui.filters.*"/>
         </includes>
      </viewerContentBinding>
